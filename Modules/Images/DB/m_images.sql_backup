CREATE TABLE `m_images` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `item_type` varchar(255) NOT NULL,  `item_id` int(10) unsigned NOT NULL,  `image` varchar(255) NOT NULL,  `order` int(10) unsigned NOT NULL,  PRIMARY KEY (`id`),  KEY `client_id` (`item_type`,`item_id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8